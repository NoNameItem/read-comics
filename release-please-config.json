{
  "plugins": ["sentence-case"],
  "release-type": "python",
  "draft": false,
  "include-v-in-tag": false,
  "packages": {
    ".": {
      "release-type": "python",
      "exclude-paths": ["docs", "compose",".envs" ]
    }
  },
