[MASTER]
load-plugins=pylint_django, pylint_celery
django-settings-module=config.settings.local
ignore=migrations

[BASIC]
good-names=i,
           j,
           k,
           e,
           _,
           q,
           qs,
           s3

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,too-complex

[DESIGN]
max-parents=13
max-attributes=20

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
